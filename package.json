  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prepare": "husky install",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:commit": "npx lint-staged",
